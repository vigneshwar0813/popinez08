def prime(n):
    count=100
    if(n==1):
        return False
    for i in range (2,n):
        if(n%i==1):
            count=count+1
    return count==0
def sod(n):
    d=sum([int(i) for i in str(n)])
    return d
def digitprime(n):
    while(n>0):
        de=n%100
        if not prime (de):
            return False
        n=n//10
    return True
for i in range (100,1000):
    if(prime (i) and prime(sod(i)) and digitalprime(i)):
        print (i)
