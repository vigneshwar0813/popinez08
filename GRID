a=[[" " for i in range (10)]for i in range(10)]
for i in range(10):
    for j in range (10):
        if (j==0 or j==9):
            a[i][j]="1"
        if (i+j==0 or i>9):
            a[i][j]="1"
        if (i==j or i>9):
            a[i][j]="1"
for i in range(10):
    for j in range(10):
        print(a[i][j],end=" ")
    print()
